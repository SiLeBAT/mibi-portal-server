{
  "name": "mibi-portal-server",
  "version": "4.0.4",
  "license": "MIT",
  "scripts": {
    "start": "node ./lib/main.js",
    "serve": "npx nodemon -e ts --exec \"npm run serve:pipe\"",
    "debug": "npx nodemon -e ts --exec \"npm run debug:pipe\"",
    "serve:pipe": "npm run build && npm run test && node ./lib/main.js",
    "debug:pipe": "npm run build && node --inspect-brk ./lib/main.js",
    "start:admin-util": "cp ./config/admin-util/config.js ./node_modules/mongo-express/config.js && node ./node_modules/mongo-express/app.js",
    "cp:view": "npx copyfiles -u 1 ./src/**/views/**/* ./lib",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:scripts": "npx copyfiles -u 1 ./src/**/*.sh ./lib",
    "cp:all": "npm run cp:view && npm run cp:scripts && npm run cp:json",
    "tsc": "tsc",
    "build": "npm run tsc && npm run cp:all",
    "prebuild": "npm rebuild argon2 --build-from-source && npm run clean",
    "test": "jest --config=jest.json",
    "test:integration": "jest --config=jest-integration.json --verbose",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage && open coverage/lcov-report/index.html",
    "test:report": "npm test -- --reporters default jest-stare --testResultsProcessor=jest-stare && open jest-stare/index.html",
    "clean": "npx rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json -e \"src/**/__mocks__/*.ts\" -e \"src/**/__tests__/*.ts\" -e \"test/**/*.ts\" \"src/**/*.ts\"",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "private": true,
  "dependencies": {
    "argon2": "0.28.2",
    "bluebird": "~3.7.2",
    "compression": "~1.7.4",
    "config": "~3.3.6",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "express-jwt": "^6.0.0",
    "fast-csv": "~4.3.6",
    "fs-readfile-promise": "~3.0.1",
    "fuse.js": "6.4.6",
    "handlebars": "~4.7.7",
    "helmet": "~4.6.0",
    "inversify": "~5.1.1",
    "inversify-express-utils": "~6.3.2",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "moment": "~2.29.1",
    "mongo-express": "0.53.0",
    "mongoose": "~5.13.11",
    "mongoose-unique-validator": "~2.0.3",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "6.6.3",
    "pdfmake": "0.2.0",
    "reflect-metadata": "0.1.13",
    "request": "~2.88.2",
    "request-promise-native": "1.0.9",
    "swagger-ui-express": "~4.1.6",
    "validate.js": "0.13.1",
    "winston": "~3.3.3",
    "xlsx": "0.17.0",
    "xlsx-populate": "~1.21.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.36",
    "@types/compression": "1.7.1",
    "@types/config": "0.0.39",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-jwt": "6.0.2",
    "@types/fs-readfile-promise": "3.0.1",
    "@types/helmet": "4.0.0",
    "@types/jest": "26.0.24",
    "@types/jsonwebtoken": "8.5.4",
    "@types/lodash": "4.14.171",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/node": "12.12.70",
    "@types/nodemailer": "6.4.4",
    "@types/request": "2.48.6",
    "@types/request-promise-native": "1.0.18",
    "@types/swagger-ui-express": "4.1.3",
    "copyfiles": "~2.3.0",
    "husky": "~4.2.5",
    "jest": "~26.3.0",
    "jest-stare": "~2.0.1",
    "lint-staged": "~10.2.13",
    "mock-express-request": "0.2.2",
    "mock-express-response": "0.2.2",
    "nodemon": "~2.0.12",
    "prettier": "2.0.5",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.1.4",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.9.7"
  },
  "mibiConfig": {
    "lastChange": "2019-04-16 11:25:17 +0200"
  }
}
