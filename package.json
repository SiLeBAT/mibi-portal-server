{
  "name": "mibi-portal-server",
  "version": "4.1.0",
  "license": "MIT",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node ./lib/main.js",
    "serve": "npx nodemon -e ts --exec \"npm run serve:pipe\"",
    "debug": "npx nodemon -e ts --exec \"npm run debug:pipe\"",
    "serve:pipe": "npm run build && node ./lib/main.js",
    "debug:pipe": "npm run build && node --inspect-brk ./lib/main.js",
    "start:admin-util": "cp ./config/admin-util/config.js ./node_modules/mongo-express/config.js && node ./node_modules/mongo-express/app.js",
    "cp:view": "npx copyfiles -u 1 ./src/**/views/**/* ./lib",
    "cp:json": "npx copyfiles -u 1 ./src/**/*.json ./lib",
    "cp:all": "npm run cp:view && npm run cp:json",
    "tsc": "tsc",
    "build": "npm run tsc && npm run cp:all",
    "prebuild": "npm run clean",
    "test": "jest --config=jest.config.js",
    "test:integration": "jest --config=jest-integration.config.js --verbose",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage && open coverage/lcov-report/index.html",
    "test:report": "npm test -- --reporters default jest-stare --testResultsProcessor=jest-stare && open jest-stare/index.html",
    "clean": "npx rimraf lib/*",
    "lint": "tslint --project tsconfig.json --fix -c tslint.json -e \"src/**/__mocks__/*.ts\" -e \"src/**/__tests__/*.ts\" -e \"test/**/*.ts\" \"src/**/*.ts\"",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "prepare": "(husky -v && husky install) || echo husky not installed"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@SiLeBAT/fg43-ne-server": "^1.1.0",
    "argon2": "0.28.5",
    "bluebird": "~3.7.2",
    "compression": "~1.7.4",
    "config": "~3.3.7",
    "cors": "~2.8.5",
    "express": "~4.18.1",
    "express-jwt": "^7.7.5",
    "fast-csv": "~4.3.6",
    "fs-readfile-promise": "~3.0.1",
    "fuse.js": "6.6.2",
    "handlebars": "~4.7.7",
    "helmet": "~5.1.0",
    "inversify": "^5.1.1",
    "inversify-express-utils": "~6.3.2",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "moment": "~2.29.4",
    "mongo-express": "0.53.0",
    "mongoose": "~5.13.14",
    "mongoose-unique-validator": "~2.0.3",
    "morgan": "~1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "6.7.5",
    "pdfmake": "0.2.5",
    "pm2": "~5.2.0",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "~4.4.0",
    "validate.js": "0.13.1",
    "winston": "~3.7.2",
    "xlsx": "0.18.5",
    "xlsx-populate": "~1.21.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.36",
    "@types/compression": "1.7.2",
    "@types/config": "0.0.41",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/fs-readfile-promise": "3.0.1",
    "@types/jest": "27.5.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.182",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/node": "12.12.70",
    "@types/nodemailer": "6.4.4",
    "@types/request": "2.48.8",
    "@types/request-promise-native": "1.0.18",
    "@types/swagger-ui-express": "4.1.3",
    "copyfiles": "~2.4.1",
    "husky": "~7.0.4",
    "jest": "~27.5.1",
    "jest-stare": "~2.4.1",
    "lint-staged": "~12.4.3",
    "mock-express-request": "0.2.2",
    "mock-express-response": "0.3.0",
    "nodemon": "~2.0.19",
    "prettier": "2.6.2",
    "request": "~2.88.2",
    "request-promise-native": "1.0.9",
    "rimraf": "~3.0.2",
    "ts-jest": "27.1.5",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.9.10"
  },
  "overrides": {
